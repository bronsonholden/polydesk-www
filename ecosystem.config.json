{
  "apps" : [
    {
      "name"        : "polydesk-www",
      "script"      : "$(npm bin -g)/http-server current/dist/polydesk-www -p 4200",
      "watch"       : true,
      "ignore_watch" : [
        "node_modules"
      ],
      "watch_options": {
        "followSymlinks": false
      },
      "env": {
        "NODE_ENV": "development"
      },
      "env_production" : {
         "NODE_ENV": "production"
      }
    }
  ]
}
